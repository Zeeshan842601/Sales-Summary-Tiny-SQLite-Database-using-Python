select * from sales;

SELECT product, SUM(quantity) AS total_qty,
       SUM(quantity * price) AS revenue
FROM sales
GROUP BY product
ORDER BY revenue DESC;

SELECT SUM(quantity) AS total_qty,
       SUM(quantity * price) AS total_revenue
FROM sales;
